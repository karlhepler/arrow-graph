<div class="Circle">
    <div class="CircleGraph CircleGraph--hidden">
        {.section collection}
        <h2 class="CircleGraph__title">{title}</h2>
        {.end}
    </div>
    <div class="CircleModal CircleModal--hidden">
        <button class="CircleModal__close-btn">&times;</button>

        {.repeated section items}
        <article class="CircleModal__section">
            <h1 class="CircleModal__title">{title}</h1>
            <div class="CircleModal__body">{body}</div>
        </article>
        {.end}

    </div>
</div>
